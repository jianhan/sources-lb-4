{
  "name": "sources-lb4",
  "version": "1.0.0",
  "description": "sources lb4",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "scrape": "npm run build && node dist/src/clis/ukfootball/index.js",
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/test\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/test/**/*.js && npm run posttest",
    "migrate": "node ./dist/src/migrate",
    "prestart": "npm run build",
    "start": "node .",
    "prepublishOnly": "npm run test",
    "jest": "jest",
    "jest-unit": "jest -c jest.config.unit.js",
    "jest-integration": "jest -c jest.config.integration.js"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist/src",
    "dist/index*",
    "src"
  ],
  "dependencies": {
    "@loopback/boot": "^1.0.10",
    "@loopback/context": "^1.4.1",
    "@loopback/core": "^1.1.4",
    "@loopback/openapi-v3": "^1.1.7",
    "@loopback/repository": "^1.1.3",
    "@loopback/rest": "^1.5.3",
    "@loopback/rest-explorer": "^1.1.6",
    "@loopback/service-proxy": "^1.0.6",
    "@types/request": "^2.48.1",
    "@types/request-promise": "^4.1.42",
    "@types/validator": "^10.9.0",
    "@types/winston": "^2.4.4",
    "app-root-path": "^2.1.0",
    "cheerio": "^1.0.0-rc.2",
    "commander": "^2.19.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "validator": "^10.11.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@loopback/build": "^1.2.0",
    "@loopback/testlab": "^1.0.4",
    "@loopback/tslint-config": "^2.0.0",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.11.2",
    "jest": "^24.0.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  }
}
